package patterns;
import java.util.*;
public class patterns_6 {
//to print pattern by giving n value as 5
//	1
//	12
//	123
//	1234
//	12345


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=0;i<=n;i++) {
			for(int j=1;j<=i;j++) {
				System.out.print(j);
			}
			System.out.println();
		}
		System.out.println();
	}

}
