package patterns;
//to print inverted half pyramid pattern by numbers  n value as 5
//1234
//123
//12
//1
import java.util.*;
public class patterns_7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=n-i+1;j++) {
				System.out.print(j);
			}
			System.out.println();
		}

	}

}
